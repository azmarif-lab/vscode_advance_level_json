{
    "editor": {
        "defaultFormatter": "esbenp.prettier-vscode",
        "hover.enabled": true,
        "stickyScroll.enabled": false,
        "minimap.enabled": true,
        "fontFamily": "CaskaydiaCove Nerd Font"
    },
    "files": {
        "autoSave": "afterDelay",
        "watcherExclude": {
            "**/node_modules/*": true,
            "**/build/*": true,
            "**/dist/*": true,
            "**/.git/*": true,
            "**/logs/*": true,
            "**/.vscode/*": true
        },
        "exclude": {
            "**/.classpath": true,
            "**/.project": true,
            "**/.settings": true,
            "**/.factorypath": true
        }
    },
    "background": {
        "enabled": true,
        "useFront": false,
        "style": {
            "content": "''",
            "pointer-events": "none",
            "position": "absolute",
            "z-index": "99999",
            "width": "40%",
            "height": "40%",
            "background-position": "center",
            "background-size": "cover",
            "background-repeat": "no-repeat",
            "opacity": 0.1,
            "left": "30%",
            "top": "10rem"
        },
        "customImages": ["https://i.postimg.cc/LXQ42V13/vscodelow.png"]
    },
    "search": {
        "exclude": {
            "**/node_modules": true,
            "**/dist": true,
            "**/build": true,
            "**/.git": true
        },
        "seedWithNearestWord": true
    },
    "powermode": {
        "enabled": false,
        "presets": "flames",
        "combo.counterSize": 0,
        "shake.enabled": false
    },
    "better-comments": {
        "highlightPlainText": true,
        "tags": [
            { "tag": "*", "color": "#000000", "bold": true, "backgroundColor": "#198754" },
            { "tag": "?", "color": "#000000", "bold": true, "backgroundColor": "#FFC107" },
            { "tag": "~", "color": "#FFFFFF", "bold": true, "backgroundColor": "#6610F2" },
            { "tag": "!", "color": "#000000", "bold": true, "backgroundColor": "#DC3545" },
            { "tag": "/", "color": "#000000", "bold": true, "backgroundColor": "#00b7ffec" },
            { "tag": "-1", "color": "#ffffff", "bold": true, "backgroundColor": "#D61355" }
        ]
    },
    "prettier": {
        "withNodeModules": true,
        "enableDebugLogs": true,
        "arrowParens": "always",
        "bracketSameLine": true,
        "bracketSpacing": true,
        "embeddedLanguageFormatting": "auto",
        "htmlWhitespaceSensitivity": "ignore",
        "insertPragma": false,
        "jsxSingleQuote": false,
        "proseWrap": "preserve",
        "quoteProps": "as-needed",
        "requirePragma": false,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all",
        "useTabs": false,
        "vueIndentScriptAndStyle": false,
        "printWidth": 125
    },
    "terminal": {
        "integrated.fontFamily": "CaskaydiaCove Nerd Font Mono",
        "integrated.defaultProfile.windows": "Git Bash",
        "integrated.defaultProfile.linux": "zsh"
    },
    "cSpell": {
        "userWords": ["arif", "azmarif", "আরিফ", "cascadia", "null"],
        "language": "en,en-GB,en-US,en-BN"
    },
    "liveServer": {
        "settings.donotShowInfoMsg": true,
        "settings.donotVerifyTags": true,
        "settings.ChromeDebuggingAttachment": true
    },
    "autoHide": {
        "autoHideSideBar": false,
        "autoHidePanel": false,
        "sideBarDelay": 0
    },
    "notebook": {
        "outline.showCodeCells": true,
        "consolidatedRunButton": true
    },
    "explorer": {
        "confirmDelete": false,
        "confirmDragAndDrop": true
    },
    "debug": {
        "console.acceptSuggestionOnEnter": "on",
        "console.fontFamily": "'Cascadia Code'",
        "showBreakpointsInOverviewRuler": true,
        "showSubSessionsInToolBar": true
    },
    "workbench": {
        "iconTheme": "vscode-icons",
        "editorAssociations": {
            "*.ico": "default"
        },
        "editor.sharedViewState": true,
        "layoutControl.enabled": false,
        "editor.empty.hint": "hidden",
        "colorTheme": "Anti-Glare - Official"
    },
    "window": {
        "customTitleBarVisibility": "never",
        "menuBarVisibility": "toggle",
        "commandCenter": true,
        "title": "A. Z. M. Arif"
    },
    "typescript": {
        "inlayHints.parameterNames.enabled": "all",
        "suggest.completeFunctionCalls": true,
        "inlayHints.parameterTypes.enabled": false,
        "format.placeOpenBraceOnNewLineForFunctions": true,
        "format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true
    },
    "javascript": {
        "inlayHints.parameterNames.enabled": "none",
        "autoClosingTags": true,
        "format.enable": true,
        "suggest.enabled": true,
        "updateImportsOnFileMove.enabled": "always",
        "format.placeOpenBraceOnNewLineForFunctions": true,
        "format.placeOpenBraceOnNewLineForControlBlocks": true
    },
    "eslint": {
        "format.enable": true,
        "debug": true,
        "lintTask.enable": true,
        "nodeEnv": ""
    },
    "json": {
        "schemaDownload.enable": true,
        "schemas": []
    },
    "code-runner": {
        "runInTerminal": true,
        "clearPreviousOutput": false
    },
    "markdown.preview.typographer": true,
    "vsicons": {
        "presets.jsonOfficial": true,
        "dontShowNewVersionMessage": true
    },
    "tabnine.experimentalAutoImports": true,
    "emmet": {
        "showAbbreviationSuggestions": true,
        "showSuggestionsAsSnippets": true,
        "triggerExpansionOnTab": true,
        "includeLanguages": {
            "javascript": "javascriptreact"
        }
    },
    "html-css-class-completion.enableEmmetSupport": true,
    "projectManager": {
        "filterOnFullPath": true,
        "openInNewWindowWhenClickingInStatusBar": true,
        "showParentFolderInfoOnDuplicates": true
    },
    "git": {
        "autofetch": true,
        "openRepositoryInParentFolders": "always"
    },
    "workbench.editor.empty.hint": "hidden"
}
